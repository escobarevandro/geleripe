### Copy this file to .env and fill values for your local environment
# Database (either set DATABASE_URL or the components below)
DATABASE_USER=root
DATABASE_PASSWORD=your_db_password
DATABASE_HOST=localhost
DATABASE_NAME=leripe
# Or use a full URL:
# DATABASE_URL="mysql://user:pass@host:3306/dbname"

# JWT secret used to sign session cookies (use a secure random string in prod)
JWT_SECRET=change_this_local_secret

NODE_ENV=development
